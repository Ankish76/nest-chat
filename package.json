{
  "name": "nest-chat",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest.config.ts",
    "test:clear": "jest --clearCache",
    "start:db": "docker compose --env-file ./.env up -d db cache",
    "db:migrate": "yarn prisma migrate dev",
    "db:migrate:prod": "yarn prisma migrate deploy",
    "stop:db": "docker compose --env-file ./.env stop",
    "codegen": "yarn prisma generate && yarn codegen:gql",
    "codegen:gql": "graphql-codegen --config codegen.ts",
    "db:seed": "ts-node --transpile-only ./src/prisma/seed.ts"
  },
  "engines": {
    "node": "^18.17.1",
    "yarn": "^1.22.10"
  },
  "prisma": {
    "schema": "./src/prisma/schema.prisma",
    "seed": "ts-node --transpile-only ./src/prisma/seed.ts"
  },
  "dependencies": {
    "@apollo/server": "^4.9.4",
    "@graphql-tools/utils": "^10.0.7",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@nestjs/apollo": "^12.0.9",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "^10.2.7",
    "@nestjs/core": "^10.0.0",
    "@nestjs/graphql": "^12.0.9",
    "@nestjs/platform-express": "^10.0.0",
    "@pothos/core": "^3.38.0",
    "@pothos/plugin-directives": "^3.10.2",
    "@pothos/plugin-prisma": "^3.61.0",
    "@pothos/plugin-prisma-utils": "^0.12.0",
    "@pothos/plugin-relay": "^3.44.0",
    "@pothos/plugin-smart-subscriptions": "^3.7.1",
    "@prisma/client": "^5.4.2",
    "cache-manager-redis-store": "^3.0.1",
    "express-session": "^1.17.3",
    "generated": "link:@pothos/plugin-prisma/generated",
    "graphql": "^16.8.1",
    "graphql-redis-subscriptions": "^2.6.0",
    "graphql-scalars": "^1.22.4",
    "graphql-ws": "^5.14.1",
    "ioredis": "^5.3.2",
    "redis": "^4.6.10",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/client-preset": "4.1.0",
    "@graphql-codegen/client-preset-swc-plugin": "^0.2.0",
    "@graphql-codegen/introspection": "4.0.0",
    "@graphql-codegen/typed-document-node": "^5.0.1",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-tools/jest-transform": "^2.0.0",
    "@jest/types": "^29.6.3",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@prisma/generator-helper": "^5.4.2",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.9",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "prisma": "^5.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "supertest-graphql": "^1.1.4",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}